<div class="row">
    <div class="col">
        <section class="registration-form">
            <div class="container h-100 pt-5 pb-5">
                <div class="row d-flex justify-content-center align-items-center h-100 row">
                    <div class="col-lg-12 col-xl-11 col">
                        <div class="card text-black">
                            <div class="card-body p-md-5">
                                <div class="row justify-content-center row">
                                    <div class="col-md-10 col-lg-6 col-xl-5 order-2 order-lg-1 col">
                                        <p class="text-center h1 fw-bold mb-5 mx-1 mx-md-4 mt-4">Sign up</p>
                                        <form class="form mx-1 mx-md-4">
                                            <div class="d-flex flex-row align-items-center mb-4"><i class="fas fa-user fa-lg me-3 fa-fw"></i>
                                                <div class="form-outline flex-fill mb-0"><input id="form3Example1c" type="text" class="form-control form-control" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;"><label for="form3Example1c" class="form-label form-label">Your Name</label></div>
                                            </div>
                                            <div class="d-flex flex-row align-items-center mb-4"><i class="fas fa-envelope fa-lg me-3 fa-fw"></i>
                                                <div class="form-outline flex-fill mb-0"><input id="form3Example3c" type="email" class="form-control form-control"><label for="form3Example3c" class="form-label form-label">Your Email</label></div>
                                            </div>
                                            <div class="d-flex flex-row align-items-center mb-4"><i class="fas fa-lock fa-lg me-3 fa-fw"></i>
                                                <div class="form-outline flex-fill mb-0"><input id="form3Example4c" type="password" class="form-control form-control" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%;"><label for="form3Example4c" class="form-label form-label">Password</label></div>
                                            </div>
                                            <div class="d-flex flex-row align-items-center mb-4"><i class="fas fa-key fa-lg me-3 fa-fw"></i>
                                                <div class="form-outline flex-fill mb-0"><input id="form3Example4cd" type="password" class="form-control form-control" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%;"><label for="form3Example4cd" class="form-label form-label">Repeat your password</label></div>
                                            </div>
                                            <div class="form-check d-flex justify-content-center mb-5"><input id="form2Example3c" type="checkbox" class="form-check-input me-2 form-check-input" value=""><label for="form2Example3" class="form-check-label form-label">I agree all statements in <a href="#!">Terms of service</a></label></div>
                                            <div class="d-flex justify-content-center mx-4 mb-3 mb-lg-4"><button type="button" class="btn btn-primary btn-lg btn btn-secondary">Register</button></div>
                                        </form>
                                    </div>
                                    <div class="col-md-10 col-lg-6 col-xl-7 d-flex align-items-center order-1 order-lg-2"><img src="/packs/media/components/assets/black-cat-943463c62b81b1574baa4e8a3a6512c0.png" class="blackcat" alt="Black cat illustration"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
